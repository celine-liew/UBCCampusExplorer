{
    "query1":{
        "WHERE": {"IS": {"courses_dept": "cpsc"} },
        "OPTIONS": {"COLUMNS": ["courses_dept", "courses_id"],
        "ORDER": "courses_id"}
    },

    "complexquery": {
        "WHERE": {
                "AND":[
                    {  "GT": { "courses_avg":90 } },
                    { "IS":{ "courses_dept":"adhe"}}
                ]
        },
        "OPTIONS": {
            "COLUMNS":["courses_dept","courses_id", "courses_avg"],
            "ORDER":"courses_avg"
        }
    },

    "d2courseQuerySimple": {
        "WHERE": {
            "GT": { "courses_avg": 98 }
        },
        "OPTIONS": {
            "COLUMNS": ["courses_title", "overallAvg"]
        },
        "TRANSFORMATIONS": {
            "GROUP": ["courses_title"],
            "APPLY": [{
                "overallAvg": {
                    "AVG": "courses_avg"
                }
            }]
        }
    }
}
