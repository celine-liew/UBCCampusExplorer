{
    "NaN": {
        "WHERE":
        {
            "AND": [
                { "IS": { "rooms_furniture": "**" }
                },
                { "GT": { "rooms_seats": "ret" }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": ["rooms_shortname", "maxSeats"],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "maxSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },

    "emptyWhere": {
        "WHERE": {},
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "year"
            ]
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept"
            ],
            "APPLY": [
                {
                    "year": {
                        "COUNT": "courses_dept"
                    }
                }
            ]
        }
    },

    "query1":{
        "WHERE": {"IS": {"courses_dept": "cpsc"} },
        "OPTIONS": {"COLUMNS": ["courses_dept", "courses_id"],
        "ORDER": "courses_id"}
    },

    "complexquery": {
        "WHERE": {
                "AND":[
                    {  "GT": { "courses_avg":90 } },
                    { "IS":{ "courses_dept":"adhe"}}
                ]
        },
        "OPTIONS": {
            "COLUMNS":["courses_dept","courses_id", "courses_avg"],
            "ORDER":"courses_avg"
        }
    },

    "d2courseQuerySimple": {
        "WHERE": {
            "GT": { "courses_avg": 98 }
        },
        "OPTIONS": {
            "COLUMNS": ["courses_title", "overallAvg"]
        },
        "TRANSFORMATIONS": {
            "GROUP": ["courses_title"],
            "APPLY": [{
                "overallAvg": {
                    "AVG": "courses_avg"
                }
            }]
        }
    },

    "coursewithNOT": {
        "WHERE": {
            "NOT" : {"LT": { "courses_avg": 98 }
        }},
        "OPTIONS": {
            "COLUMNS": ["courses_avg", "courses_instructor"]
        },
        "TRANSFORMATIONS": {
            "GROUP": ["courses_avg", "courses_instructor"],
            "APPLY": []
        }
    },

