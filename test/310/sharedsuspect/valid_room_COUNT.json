{
    "title": "room query",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 250
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "rooms_seats",
                "countNum"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "countNum"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname",
                "rooms_seats"
            ],
            "APPLY": [
                {
                    "countNum": {
                        "COUNT": "rooms_number"
                    }
                }
            ]
        }
    },
