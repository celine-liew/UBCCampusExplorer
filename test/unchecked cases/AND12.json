{
    "title": "SELECT dept, id, avg, year, pass WHERE fail == 0 && avg == 95.9 && year < 2017 ORDER BY year",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "EQ": {
                        "courses_fail": 0
                    }
                },
                {
                    "EQ": {
                        "courses_avg": 95.9
                    }
                },
                {
                    "LT": {
                        "courses_year": 2017
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id",
                "courses_avg",
                "courses_year",
                "courses_pass"
            ],
            "ORDER": "courses_year"
        }
    },
    "isQueryValid": true,
