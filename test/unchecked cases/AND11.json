{
    "title": "SELECT dept, id, avg WHERE fail < 2 && avg == 95 && year == 2016 ORDER BY avg",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "LT": {
                        "courses_fail": 2
                    }
                },
                {
                    "EQ": {
                        "courses_avg": 95
                    }
                },
                {
                    "EQ": {
                        "courses_year": 2016
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
    "result": [
        { "courses_dept": "cnps", "courses_id": "535", "courses_avg": 95 },
        { "courses_dept": "econ", "courses_id": "516", "courses_avg": 95 }
    ]
}
