{
    "title": "room given example",

    "query": {
        "WHERE": {
            "AND": [{
                "IS": {
                    "rooms_furniture": "*Tables*"
                }
            }, {
                "GT": {
                    "rooms_seats": 300
                }
            }]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "maxSeats",
                "sumSeats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": ["maxSeats","sumSeats"]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": ["rooms_shortname","rooms_href"],
            "APPLY": [{
                "maxSeats": {
                    "MAX": "rooms_seats"
                }
            },{
                "sumSeats": {
                    "SUM": "rooms_seats"
                }
            }]
        }
    },
    "isQueryValid": true,
    "result":
    [{"rooms_shortname":"OSBO","maxSeats":442,"sumSeats":442},
        {"rooms_shortname":"HEBB","maxSeats":375,"sumSeats":375},
        {"rooms_shortname":"LSC","maxSeats":350,"sumSeats":350},
        {"rooms_shortname":"LSC","maxSeats":350,"sumSeats":350}]
}
