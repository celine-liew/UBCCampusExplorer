{
    "title": "SELECT dept, id, avg WHERE instructor = 'grow, laura' || dept! = 'math' &&  avg == 97",

    "query": {
        "WHERE": {
            "AND": [
                {
                    "OR": [
                        {
                            "IS": {
                                "courses_instructor": "grow, laura"
                            }
                        },
                        {
                            "NOT": {
                                "IS": {
                                    "courses_dept": "math"
                                }
                            }
                        }
                    ]
                },
                {
                    "EQ": {
                        "courses_avg": 97
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
    "result":[
        {"courses_dept":"crwr","courses_id":"599","courses_avg":97},
        {"courses_dept":"epse","courses_id":"534","courses_avg":97},
        {"courses_dept":"psyc","courses_id":"549","courses_avg":97}
    ]
}
