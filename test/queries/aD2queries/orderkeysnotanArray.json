{
    "title": "room given example",

    "query": {
        "WHERE": {
            "AND": [{
                "IS": {
                    "rooms_furniture": "*Tables*"
                }
            }, {
                "GT": {
                    "rooms_seats": 300
                }
            }]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": "maxSeats"
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": ["rooms_shortname","rooms_lat"],
            "APPLY": [{
                "maxSeats": {
                    "MAX": "rooms_seats"
                },
                "fasdfsa": {
                    "MAX": "rooms_lon"
                }
            }]
        }
    },
    "isQueryValid": false,
    "result":  "InsightError"
}
