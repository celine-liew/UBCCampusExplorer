{
    "title": "room given example",

    "query": {
        "WHERE": {
            "AND": [{
                "IS": {
                    "rooms_furniture": "*Tables*"
                }
            }, {
                "GT": {
                    "rooms_seats": 100
                }
            }]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_type",
                "COUNT",
                "sumSeats",
                "rooms_href"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": ["COUNT","sumSeats"]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": ["rooms_shortname","rooms_href"],
            "APPLY": [{
                "COUNT": {
                    "COUNT": "rooms_type"
                }
            },{
                "sumSeats": {
                    "SUM": "rooms_seats"
                }
            },{
                "sumSeatsd": {
                    "MIN": "rooms_seats"
                }
            }]
        }
    },
    "isQueryValid": false,
    "result":"InsightError"
}
